{
  "name": "bananasplit-js",
  "version": "2.0.0",
  "description": "A bananasplit app",
  "main": "app.js",
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/app.ts --ignore tests",
    "start": "node -r module-alias/register dist/app.js",
    "build": "tsc",
    "build:database": "npx sequelize db:create && npx sequelize db:migrate && npx sequelize db:seed:all",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:coverage": "jest --coverage --runInBand",
    "test:cache": "jest --clearCache"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/diegoulloao/bananasplit-express-template"
  },
  "keywords": [
    "express",
    "nodejs",
    "sequelize",
    "jest",
    "typescript",
    "graphql",
    "apollo",
    "template"
  ],
  "author": "@diegoulloao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/diegoulloao/bananasplit-express-template/issues"
  },
  "homepage": "",
  "dependencies": {
    "@types/bluebird": "^3.5.32",
    "@types/node": "^14.11.2",
    "@types/validator": "^13.1.0",
    "apollo-server": "^2.18.1",
    "apollo-server-express": "^2.18.1",
    "boxen": "^4.2.0",
    "chalk": "^4.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "i": "^0.3.6",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "npm": "^6.14.8",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-strict-mode": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "@types/express": "^4.17.8",
    "@types/faker": "^5.1.2",
    "@types/jest": "^26.0.14",
    "@types/morgan": "^1.9.1",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "^2.0.10",
    "alias-hq": "^5.1.6",
    "babel-plugin-add-module-exports": "^1.0.4",
    "faker": "^5.1.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "npm-check-updates": "^9.0.3",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3"
  },
  "_moduleAliases": {
    "@bananasplit-js": "dist/providers/core",
    "@services": "dist/providers/services",
    "@providers": "dist/providers",
    "@database": "dist/database",
    "@apollo": "dist/apollo",
    "@schemas": "dist/apollo/schemas",
    "@resolvers": "dist/apollo/resolvers",
    "@settings": "dist/settings",
    "@middlewares": "dist/middlewares",
    "@controllers": "dist/app/controllers",
    "@models": "dist/app/models",
    "@routes": "dist/app/routes",
    "@helpers": "dist/app/helpers"
  }
}

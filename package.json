{
  "name": "bananasplit-js",
  "version": "1.2.0",
  "description": "A bananasplit app",
  "main": "app.js",
  "scripts": {
    "dev": "nodemon src/app.ts --exec ts-node --ignore tests",
    "start": "node dist/app.js",
    "build": "tsc",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:coverage": "jest --coverage --runInBand",
    "test:cache": "jest --clearCache"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/diegoulloao/bananasplit-express-template"
  },
  "keywords": [
    "express",
    "nodejs",
    "sequelize",
    "jest",
    "typescript",
    "graphql",
    "apollo",
    "template"
  ],
  "author": "@diegoulloao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/diegoulloao/bananasplit-express-template/issues"
  },
  "homepage": "",
  "dependencies": {
    "@types/bluebird": "^3.5.30",
    "@types/node": "^13.13.4",
    "@types/validator": "^13.0.0",
    "apollo-server": "^2.12.0",
    "apollo-server-express": "^2.12.0",
    "boxen": "^4.2.0",
    "chalk": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.0.0",
    "i": "^0.3.6",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "mysql2": "^2.1.0",
    "npm": "^6.14.4",
    "sequelize": "^5.21.7",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/faker": "^4.1.11",
    "@types/jest": "^25.2.1",
    "@types/morgan": "^1.9.0",
    "@types/sequelize": "^4.28.8",
    "@types/supertest": "^2.0.9",
    "alias-hq": "^5.1.6",
    "faker": "^4.1.0",
    "jest": "^25.5.4",
    "nodemon": "^2.0.3",
    "npm-check-updates": "^4.1.2",
    "supertest": "^4.0.2",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  },
  "_moduleAliases": {
    "@bananasplit-js": "src/providers/core",
    "@services": "src/providers/services",
    "@providers": "src/providers",
    "@database": "src/database",
    "@graphql": "src/graphql",
    "@schemas": "src/graphql/schemas",
    "@resolvers": "src/graphql/resolvers",
    "@settings": "src/settings",
    "@middlewares": "src/middlewares",
    "@controllers": "src/app/controllers",
    "@models": "src/app/models",
    "@routes": "src/app/routes",
    "@helpers": "src/app/helpers"
  }
}
